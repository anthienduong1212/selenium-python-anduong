{
  "chrome": {
    "args": ["--disable-gpu"],
    "prefs": {
    },
    "capabilities": {
      "pageLoadStrategy": "eager"
    }
  },
  "edge": {
    "args": ["--inprivate"],
    "prefs": {
      "download.default_directory": "C:/Downloads"
    },
    "capabilities": {
    },
    "ms:edgeOptions": {
      "excludeSwitches": ["enable-logging"]
    }
  },
  "firefox": {
    "prefs": {
    },
    "capabilities": {
    },
    "moz:firefoxOptions": {
      "log": {"level": "trace"}
    }
  }
}
